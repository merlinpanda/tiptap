{
  "name": "@stument/react",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c",
    "build:watch": "npm run clean && rollup -c -w"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "umd": "dist/index.umd.js",
  "types": "dist/packages/@stument-react/src/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/packages/@stument-react/src/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/css/index.css",
    "./styles/*": "./dist/css/*",
    "./sass/*": "./src/scss/*"
  },
  "dependencies": {
    "@mantine/carousel": "^7.0.0",
    "@mantine/code-highlight": "^7.0.0",
    "@mantine/core": "^7.0.0",
    "@mantine/dates": "^7.0.0",
    "@mantine/dropzone": "^7.0.0",
    "@mantine/form": "^7.0.0",
    "@mantine/hooks": "^7.0.0",
    "@mantine/modals": "^7.0.0",
    "@mantine/notifications": "^7.0.0",
    "@mantine/nprogress": "^7.0.0",
    "@mantine/spotlight": "^7.0.0",
    "@mantine/tiptap": "^7.0.0",
    "@stument/extension-mention": "^1.0.0",
    "@tabler/icons-react": "^2.34.0",
    "@tiptap/extension-link": "^2.1.10",
    "@tiptap/react": "^2.1.11",
    "@tiptap/starter-kit": "^2.1.10",
    "@tiptap/suggestion": "^2.1.11",
    "dayjs": "^1.11.10",
    "embla-carousel-react": "^8.0.0-rc14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.22",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.16",
    "cssnano": "^6.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "sass": "^1.68.0"
  },
  "browserslist": [
    "defaults",
    "not ie < 8",
    "last 2 versions",
    "> 1%",
    "iOS 7",
    "last 3 iOS versions"
  ]
}
